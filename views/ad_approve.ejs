<br><br>
<div class = "navbar">
    NAV_BAR
    <br>
    <form action="/ad_homepage" method="GET" style="display: inline-block;">
        <input type="button" onclick="location.href='/ad_homepage';" value="HOME"/>
    </form>
    <form action="/requests" method="POST">
        <input type = "submit" value = "SEE REQUESTS.."/>
    </form>
    <form action="/addremovebooks" method="POST">
        <input type = "submit" value = "Add/Remove Books"/>
    </form>
    <form action="/logout" method="GET" style="display: inline-block;">
        <input type="button" onclick="location.href='/logout';" value="Logout"/>
    </form>
</div>
<br>
ALL CHECKOUT-REQ : 
    <table>
        <%
            data.forEach(function(item) {
        %>    
       <tr>
           <td>
                <form action="/approverequests" method="POST">
                <%=item.id%><%=item.title%><%=item.user_id%><br>
                <input type="text" name="reqid" value= '<%= item.id %>' style="display:none;">
                <input type="text" name="title" value= '<%= item.title %>' style="display:none;">
                <input type="submit" value="approve">
                </form>
                <form action="/denyrequests" method="POST">
                    <input type="text" name="reqid" value= '<%= item.id %>' style="display:none;">
                    <input type="text" name="title" value= '<%= item.title %>' style="display:none;">
                    <input type="submit" value="deny">
                </form>
                </td>
            </td>
       </tr>
       <% }) %>
    </table>
    <br><br>